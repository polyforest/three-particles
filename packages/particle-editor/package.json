{
    "name": "@polyforest/three-particles-editor",
    "version": "0.6.0",
    "private": true,
    "scripts": {
        "clean": "rimraf dist",
        "lint": "npm run prettier:check && eslint .",
        "lint:write": "npm run prettier:write && eslint . --fix",
        "prettier:check": "prettier . --ignore-path ../../.prettierignore --cache --check",
        "prettier:write": "prettier . --ignore-path ../../.prettierignore --cache --write",
        "typecheck": "tsc --noEmit",
        "build": "vite build && npm run typecheck",
        "dev": "vite"
    },
    "author": "Nicholas Bilyk",
    "license": "ISC",
    "description": "React-based editor for three-particles",
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.0",
        "@mui/material": "^7.3.1",
        "loglevel": "^1.9.2",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router-dom": "^7.8.2",
        "three-particles": "*",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/loglevel": "^1.5.4",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "rimraf": "^4.4.1"
    }
}
