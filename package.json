{
    "name": "three-particles-composite",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "esbuild": "^0.25.1",
        "eslint": "^9.39.1",
        "globals": "^16.5.0",
        "http-server": "^14.1.1",
        "jest": "^30.2.0",
        "lerna": "^9.0.3",
        "prettier": "^3.7.4",
        "ts-jest": "^29.4.6",
        "tsx": "^4.19.3",
        "typescript": "^5.9.3",
