{
  "tags": {
	"allowUnknownTags": true,
	"dictionaries": ["jsdoc","closure"]
  },
  "source": {
	"include": ["src"],
	"includePattern": ".+\\.js?$",
	"excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
	"plugins/markdown"
  ],
  "templates": {
	"cleverLinks": true,
	"monospaceLinks": true
  },
  "opts": {
	"destination": "jsdocs",
	"recurse": true,
	"readme": "README.md"
  }
}